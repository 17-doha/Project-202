                    <a href="#book-a-table" class="btn-book-a-table">Order Now</a>
                </div>
            </div>
            <div class="col-lg-5 order-1 order-lg-2 text-center text-lg-start">
                <img src="~/img/WhatsApp_Image_2023-12-23_at_14.52.20-removebg-preview (1).png" class="img-fluid" alt="" data-aos="zoom-out" data-aos-delay="300">
            </div>
        </div>
    </div>
</section><!-- End Hero Section -->
